enum AuthProvider {
    GOOGLE
    DEFAULT
}

model User {
    id           String       @id @default(uuid())
    name         String
    email        String       @unique
    password     String?
    registerCode String?
    role         Role?
    authProvider AuthProvider @default(DEFAULT)
    createdAt    DateTime     @default(now())
    updatedAt    DateTime     @updatedAt
}
